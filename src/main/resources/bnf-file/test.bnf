<question> := <simple-filter>;

<simple-filter> := <all-int-measure-column> <symbol> <number> |
                   <all-int-measure-column>;

<symbol> := <bool-symbol> |
            <math-symbol>;

<all-int-measure-column> := <int-measure-column> |
                            <table-int-measure-column>;

<bool-symbol> := > | < | = | !=;
<math-symbol> := + | - | * | /;
<integer> := 1;
<number> := 1;